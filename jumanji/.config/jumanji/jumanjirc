# # jumanjirc

#set developer_extras true
#script /home/anthony/.config/jumanji/scripts/hints.js

# JUMANJI SETTINGS

set adblock true
set homepage https://www.duckduckgo.com
set download-dir ~/Downloads/
set download-command "wget --load-cookies ~/.config/jumanji/cookies '%s' -O %s"
set show-scrollbars true
set save-session-at-exit true
set load-session-at-startup true

# APPEARANCE SETTINGS 

# Defines the color that is used for command line completion entries
set completion-bg "#ededed"
set completion-fg "#292929"

# Defines the color that is used for command line completion group
# elements
set completion-group-bg "#ededed"
set completion-group-fg "#292929"

# defines the default color
set default-bg "#ededed"
set default-fg "#292929"

# defines the background color of the inputbar
set inputbar-bg "#ededed"
set inputbar-fg "#292929"

# Defines the color for an error notification
set notification-error-bg "#ededed"
set notification-error-fg "#292929"

# Defines the color for a warning notification
set notification-warning-bg "#ededed"
set notification-warning-fg "#292929"

# Defines the color for the a tab
set tabbar-bg "#ededed"
set tabbar-fg "#292929"

# Defines the color for the focused tab
set tabbar-focus-bg "#ededed"
set tabbar-focus-fg "#aa2e00"

# Defines the color of the status bar
set statusbar-bg "#ededed"
set statusbar-fg "#292929"

## FONTS
set font "Merriweather Sans  11"
set hint-css true

# SHORTCUTS

# map f sc_hints

#  SEARCH ENGINES

searchengine sp https://startpage.com/do/search?query=%s
searchengine duck http://duckduckgo.com/?q=%s
searchengine go https://www.google.com/search?q=%s
searchengine arch https://wiki.archlinux.org/?search=%s
searchengine wiki https://en.wikipedia.org/w/?search=%s

